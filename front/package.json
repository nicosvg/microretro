{
	"name": "front",
	"version": "0.0.1",
	"scripts": {
		"compose": "bun install && vite dev --host 0.0.0.0",
		"dev": "vite dev",
		"start": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && oxlint",
		"oxlint": "oxlint",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^5.0.0",
		"@neoconfetti/svelte": "^2.0.0",
		"@skeletonlabs/skeleton": "^4.11.0",
		"@skeletonlabs/skeleton-svelte": "^4.11.0",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.2.9",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.17.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.3",
		"@tailwindcss/typography": "^0.5.14",
		"@types/node": "^22.7.5",
		"globals": "^15.0.0",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.2.6",
		"prettier-plugin-tailwindcss": "^0.6.13",
		"svelte": "^5.19.3",
		"svelte-check": "^4.1.4",
		"svelte-confetti": "^2.3.1",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.5.0",
		"vite": "^6.0.11",
		"vitest": "^2.0.0"
	},
	"type": "module",
	"overrides": {
		"vite": {
			"rollup": "npm:@rollup/wasm-node"
		}
	},
	"dependencies": {
		"@fontsource/quicksand": "^5.1.1",
		"@tailwindcss/vite": "^4.1.11",
		"@thisux/sveltednd": "^0.0.20",
		"lorem-ipsum": "^2.0.8",
		"lucide-svelte": "^0.474.0"
	}
}